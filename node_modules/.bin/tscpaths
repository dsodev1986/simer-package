#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/studio/node_modules/.pnpm/tscpaths@0.0.9/node_modules/tscpaths/cjs/node_modules:/home/user/studio/node_modules/.pnpm/tscpaths@0.0.9/node_modules/tscpaths/node_modules:/home/user/studio/node_modules/.pnpm/tscpaths@0.0.9/node_modules:/home/user/studio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/studio/node_modules/.pnpm/tscpaths@0.0.9/node_modules/tscpaths/cjs/node_modules:/home/user/studio/node_modules/.pnpm/tscpaths@0.0.9/node_modules/tscpaths/node_modules:/home/user/studio/node_modules/.pnpm/tscpaths@0.0.9/node_modules:/home/user/studio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/tscpaths@0.0.9/node_modules/tscpaths/cjs/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/tscpaths@0.0.9/node_modules/tscpaths/cjs/index.js" "$@"
fi
